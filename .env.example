# WAHA Configuration
# Copy this file to .env and update with your values

# API Keys (generate your own secure keys)
WAHA_API_KEY=sha512:your_hashed_api_key_here
WAHA_API_KEY_PLAIN=your_plain_api_key_here

# Application Settings
WAHA_APPS_ENABLED=true

# Dashboard Credentials (change these!)
WAHA_DASHBOARD_USERNAME=admin
WAHA_DASHBOARD_PASSWORD=admin

# Docker Configuration
CONTAINER_NAME=waha
HOST_PORT=3000
CONTAINER_PORT=3000
SESSIONS_PATH=./sessions
